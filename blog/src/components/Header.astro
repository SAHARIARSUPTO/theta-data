---
import { Image } from 'astro:assets';
---


<!-- Fonts & CSS -->
<link
  href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
  rel="stylesheet"
/>
<link href="/css/custom-bootstrap.css" rel="stylesheet" />
<link href="/css/header.css" rel="stylesheet" />
<link
  rel="stylesheet"
  href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
  crossorigin="anonymous"
/>
<link href="/css/effects.css" rel="stylesheet" />
<link href="/css/pricing-table.css" rel="stylesheet" />
<link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet" />

<!-- Header -->
<div class="header-wrapper">
  <nav class="navbar navbar-expand-lg navbar-clean py-3 shadow-sm bg-white">
    <div class="container">
      <!-- Brand Name -->
      <a class="navbar-brand fw-bold fs-4 text-dark" href="/index.html">
        ThetaData
      </a>

      <!-- Mobile Toggler -->
      <button
        class="navbar-toggler border-0"
        type="button"
        data-bs-toggle="collapse"
        data-bs-target="#navbarContent"
        aria-controls="navbarContent"
        aria-expanded="false"
        aria-label="Toggle navigation"
      >
        <span class="navbar-toggler-icon"></span>
      </button>

      <!-- Navbar Links -->
      <div class="collapse navbar-collapse" id="navbarContent">
        <ul class="navbar-nav mx-auto mb-2 mb-lg-0 align-items-lg-center">
          <!-- Pricing -->
          <li class="nav-item">
            <a class="nav-link px-3" href="/pages/pricing-all.html">
              <i class="fas fa-dollar-sign me-1"></i> Pricing
            </a>
          </li>

          <!-- Blogs -->
          <li class="nav-item">
            <a class="nav-link px-3" href="/blog/dist">
              <i class="fas fa-blog me-1"></i> ThetaData Blogs
            </a>
          </li>

          <!-- Discord -->
          <li class="nav-item">
            <a class="nav-link px-3" href="https://discord.thetadata.us/" target="_blank">
              <i class="fab fa-discord me-1"></i> Discord
            </a>
          </li>

          
        </ul>

        <!-- Signup Button -->
        <a href="/pages/signup.html" class="btn btn-get-started ms-lg-3">
          <i class="fas fa-user-plus me-1"></i> Signup
        </a>
      </div>
    </div>
  </nav>
</div>

<!-- JS -->
<script is:inline>
  import('https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js');
</script>
<script is:inline src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
